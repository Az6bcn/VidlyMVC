@model Vidly.ViewModels.RandomMovieViewModel

@{
    ViewBag.Title = "Random";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<p> Movie:</p>
<h2>@Model.movie.Name</h2>


<h4>Customers</h4>
<ul>
    @foreach (var item in @Model.Customers)
    {
        <li>
            @item.Name
        </li>
    }
</ul>